{
  "name": "immotus",
  "description": "A Vite Static Site Generator using: React, TypeScript, Material UI and React Router",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "ts-node server",
    "build": "yarn run build:static && yarn run build:client && yarn run build:server",
    "build:static": "vite build --outDir dist/static",
    "build:client": "vite build --outDir dist/client",
    "build:server": "vite build --ssr src/entry-server.tsx --outDir dist/server",
    "generate": "yarn run build && tsc prerender.ts; node prerender",
    "serve": "cross-env NODE_ENV=production ts-node server.ts",
    "serve:dev": "cross-env ts-node server.ts",
    "static": "yarn run generate && ts-node static-server.ts",
    "debug": "node --inspect-brk server",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "lint": "npx eslint . --ext .ts,.tsx",
    "lint:fix": "npx eslint . --ext .ts,.tsx --fix"
  },
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@mui/material": "^5.4.2",
    "@mui/styled-engine": "npm:@mui/styled-engine-sc@^5.1.0",
    "@mui/styled-engine-sc": "^5.4.2",
    "@mui/styles": "^5.4.2",
    "cross-env": "^7.0.3",
    "express": "^4.17.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.1",
    "storybook-addon-material-ui": "^0.9.0-alpha.24",
    "styled-components": "^5.3.3"
  },
  "resolutions": {
    "@mui/styled-engine": "npm:@mui/styled-engine-sc@latest"
  },
  "alias": {
    "@mui/styled-engine": "@mui/styled-engine-sc"
  },
  "devDependencies": {
    "@babel/core": "^7.17.4",
    "@storybook/addon-actions": "^6.4.19",
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/addon-interactions": "^6.4.19",
    "@storybook/addon-links": "^6.4.19",
    "@storybook/react": "^6.4.19",
    "@storybook/testing-library": "^0.0.9",
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.18",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@types/styled-components": "^5.1.23",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@vitejs/plugin-react": "^1.0.7",
    "babel-loader": "^8.2.3",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.4",
    "vite": "^2.8.0"
  }
}
